programa {
  funcao inicio() {
    real salario, abono, novosalario
    
    escreva("Digite o valor do Salário: ")
    leia(salario)
    escreva("Digite o valor do Abono: ")
    leia(abono)
    novosalario = salario + abono
    escreva("O seu novo salário é: " + novosalario)
  }
}
